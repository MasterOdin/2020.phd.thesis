@inproceedings{van_den_briel_effective_2004,
	address = {San Jose, CA},
	series = {{AAAI}'04},
	title = {Effective {Approaches} for {Partial} {Satisfaction} ({Over}-subscription) {Planning}},
	isbn = {978-0-262-51183-4},
	url = {http://dl.acm.org/citation.cfm?id=1597148.1597240},
	abstract = {In many real world planning scenarios, agents often do not have enough resources to achieve all of their goals. Consequently, they are forced to find plans that satisfy only a subset of the goals. Solving such partial satisfaction planning (PSP) problems poses several challenges, including an increased emphasis on modeling and handling plan quality (in terms of action costs and goal utilities). Despite the ubiquity of such PSP problems, very little attention has been paid to them in the planning community. In this paper, we start by describing a spectrum of PSP problems and focus on one of the more general PSP problems, termed PSP NET BENEFIT. We develop three techniques, (i) one based on integer programming, called OptiPlan, (ii) the second based on regression planning with reachability heuristics, called AltAltps, and (iii) the third based on anytime heuristic search for a forward state-space heuristic planner, called Sapaps. Our empirical studies with these planners show that the heuristic planners generate plans that are comparable to the quality of plans generated by OptiPlan, while incurring only a small fraction of the cost.},
	urldate = {2019-07-16},
	booktitle = {Proceedings of the 19th {National} {Conference} on {Artifical} {Intelligence}},
	publisher = {AAAI Press},
	author = {van den Briel, Menkes and Sanchez, Romeo and Do, Minh B. and Kambhampati, Subbarao},
	year = {2004},
	note = {event-place: San Jose, California},
	pages = {562--569}
}

